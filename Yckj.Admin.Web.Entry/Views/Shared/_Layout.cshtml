<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewData["Title"]</title>

    <style>
        *{
            margin:0px;
            padding:0px;
        }
        html,body
        {
            width:100%;
            height:100%;
            background:#f8f8f8
        }

        a {
            text-decoration: none;
            color: black;
            -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
            -webkit-user-select: none;
            -moz-user-focus: none;
            -moz-user-select: none;
        }

            a:link {
                color: black;
            }
    </style>
</head>
<body>
    @RenderBody()
    <script src="~/style/element/vue.js"></script>
    <script src="~/style/element/index.js"></script>
</body>
</html>