.privacy-container{padding:40rpx;min-height:100vh;box-sizing:border-box;background:#f8f8f8}.privacy-container .header{margin-bottom:40rpx}.privacy-container .title{font-size:36rpx;font-weight:700;color:#333;display:block;margin-bottom:10rpx}.privacy-container .subtitle{font-size:26rpx;color:#666}.privacy-container .content{background:#fff;border-radius:12rpx;padding:30rpx}.privacy-container .section{margin-bottom:30rpx}.privacy-container .section:last-child{margin-bottom:0}.privacy-container .section-title{font-size:30rpx;font-weight:700;color:#333;margin-bottom:20rpx}.privacy-container .section-text{font-size:28rpx;color:#666;line-height:1.6;display:block;margin-bottom:20rpx}.privacy-container .list-item{display:flex;margin-bottom:10rpx}.privacy-container .item-dot{font-size:28rpx;color:var(--color-primary);margin-right:10rpx}.privacy-container .item-text{font-size:28rpx;color:#666;line-height:1.6;flex:1}.privacy-container .contact-info{font-size:28rpx;color:#666;display:block;margin-top:10rpx}