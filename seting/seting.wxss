.seting-container{padding:40rpx;min-height:100vh}.seting-container .header{margin-bottom:40rpx}.seting-container .title{font-size:36rpx;font-weight:700;color:var(--color-text);display:block;margin-bottom:10rpx}.seting-container .subtitle{font-size:26rpx;color:var(--color-text-light)}.seting-container .settings-group{background:#fff;border-radius:12rpx;padding:20rpx 30rpx;margin-bottom:30rpx}.seting-container .settings-title{font-size:28rpx;color:#333;font-weight:700;margin-bottom:20rpx}.seting-container .settings-item{display:flex;justify-content:space-between;align-items:center;padding:20rpx 0;border-bottom:1px solid #f5f5f5}.seting-container .settings-item:last-child{border-bottom:none}.seting-container .item-label{font-size:28rpx;color:#333}.seting-container .picker-value{font-size:28rpx;color:#666}.seting-container .item-action{font-size:28rpx;color:var(--color-primary)}.seting-container .danger .item-action{color:var(--color-primary)}.seting-container .version-info{text-align:center;margin-top:60rpx;font-size:24rpx;color:#999}